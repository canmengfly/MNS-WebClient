
<!-- 1. Create Queue -->
<div id="1" class="menu-div" style="display:none">
    <form method="POST" action="/mnsqueue-queue">
        <span>Create Queue</span><hr><br>
        <label>Queue Name:</label>
        <input placeholder="Queue Name" id="queuename" type="text" name="queuename" value="<%= ui.def_mnsqueuename %>" required><br><br>
        <input type="submit" value="Create Queue"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[1] %></textarea>
    </form>
</div>

<!-- 2. List all Queues -->    
<div id="2" class="menu-div" style="display:none">
    <form  method="GET" action="/mnsqueue-queue/list">
        <span>List Queues</span><hr><br><br>
        <input type="submit" value="List Queue URLs"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[2] %></textarea>
    </form>
</div>

<!-- 3. Get Queue Attributes -->
<div id="3" class="menu-div" style="display:none">
    <form  method="GET" action="/mnsqueue-queue/attributes">
        <span>Get Queue Attributes</span><hr><br>
        <label>Queue Name:</label>
        <input placeholder="Queue Name" id="queuename" type="text" name="queuename"value="<%= ui.def_mnsqueuename %>"required><br><br>
        <input type="submit" value="Get Queue Attributes"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[3] %></textarea>
    </form>
</div>

<!-- 4. Post Message to Queue -->    
<div id="4" class="menu-div" style="display:none">
    <form method="POST" action="/mnsqueue-queue/message">
        <span>Post Message To Queue</span><hr><br>
        <label>Queue Name:</label>
        <input placeholder="Queue Name" id="queuename" type="text" name="queuename" value="<%= ui.def_mnsqueuename %>" required><br><br>
        <label>Message:</label>
        <input placeholder="Message" id="message" type="text" name="message" value="<%= ui.def_message %>" required><br><br>
        <input type="submit" value="Sent Message"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[4] %></textarea>
    </form>
</div>

<!--5. Get Message from Queue -->    
<div id="5" class="menu-div" style="display:none">
    <form method="GET" action="/mnsqueue-queue/message">
        <span>Get Message From Queue</span><hr><br>
        <label>Queue Name:</label>
        <input placeholder="Queue Name" id="queuename" type="text" name="queuename" value="<%= ui.def_mnsqueuename %>" required><br><br>
        <input type="submit" value="Retrieve Message"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[5] %></textarea>
    </form>
</div>

<!-- 6. Delete Message from Queue -->    
<div id="6" class="menu-div" style="display:none">
    <form method="POST" action="/mnsqueue-queue/message/delete">
    <span>Delete Message From Queue</span><hr><br>
        <label>Queue Name:</label>
        <input placeholder="Queue Name" id="queuename" type="text" name="queuename" value="<%= ui.def_mnsqueuename %>" required> <br><br>
        <label>Message Receipt Handle:</label>
        <input placeholder="Message Receipt Handle" id="messagehandle" type="text" name="messagehandle" value="<%= ui.def_msghandle %>" required><br><br>
        <input type="submit" value="Delete Message"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[6] %></textarea>
    </form>
</div>


<!-- 7. Delete Queue (deletes queue and all messsages) --> 
<div id="7" class="menu-div" style="display:none">   
    <form method="POST" action="/mnsqueue-queue/delete">
        <span>Delete Queue</span><hr><br>
        <label>Queue Name:</label>
        <input placeholder="Queue Url" id="queuename" type="text" name="queuename" value="<%= ui.def_mnsqueuename %>" required><br><br>
        <input type="submit" value="DELETE Queue"><br><br>
        <label>Response:</label>
        <textarea name="response" id ="response" rows=25 readonly><%= ui.data[7] %></textarea>
    </form>
</div>